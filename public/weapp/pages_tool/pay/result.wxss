.consume-box{padding:20rpx;margin-top:60rpx;width:100%}.consume-box .consume-line{background:#bfbfbf;float:left;width:15%;text-align:center;height:2rpx;box-sizing:border-box;margin:0 15% 0 5%}.consume-box .consume-line:first-child{margin:0 5% 0 15%}.consume-box .consume-head-text{width:30%;float:left;text-align:center;margin-top:-26rpx;color:#303133;font-size:28rpx}.consume-box .consume-item{background-color:#fff;float:left;width:29%;margin-left:3.25%;text-align:center;margin-top:45rpx;padding:20rpx 0;color:#303133;font-size:28rpx}.consume-box .consume-item .consume-value{font-size:32rpx;font-weight:700}.consume-box .consume-item .consume-btn{color:#fff;width:100rpx;border-radius:10rpx;margin:4rpx auto}.consume-box .consume-remark{color:#909399;font-size:24rpx;padding:10rpx 20rpx}.clear{clear:both}.container{display:flex;flex-direction:column;align-items:center}.container .result-box{padding-top:94rpx;display:flex;flex-direction:column;align-items:center;background-color:#fff;width:100%;padding-bottom:40rpx}.container .result-image{width:160rpx;height:120rpx}.container .msg{font-size:32rpx;margin-top:30rpx;height:32rpx;line-height:32rpx;font-weight:700}.container .pay-amount{font-size:30rpx;margin-top:70rpx;font-weight:600;height:50rpx;line-height:50rpx;color:var(--main-color)}.container .pay-amount text{font-size:50rpx}.container .action{width:100%;height:80rpx;display:flex;justify-content:center;box-sizing:border-box;margin-top:90rpx}.container .action .btn{font-size:28rpx;width:200rpx;height:70rpx;line-height:66rpx;text-align:center;border-radius:10rpx;border:1px solid #909399;box-sizing:border-box}.container .action .btn:last-child{margin-left:100rpx}