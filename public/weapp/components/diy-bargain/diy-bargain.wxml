<block wx:if="{{dataList.length}}"><view class="diy-bargain" style="{{'background-color:'+(value.backgroundColor)+';'+('padding:'+(value.padding*2+'rpx 0rpx')+';')}}" data-theme="{{themeStyle}}"><block wx:if="{{value.list}}"><view class="diy-bargain-title" hover-class="none" data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e"><view class="seckill-title"><block wx:if="{{value.list.imageUrl}}"><image class="left-img" src="{{$root.g0}}"></image></block><text class="seckill-title-name" style="{{'color:'+(value.titleTextColor)+';'}}">{{value.list.title}}</text></view><view class="seckill-more" style="{{'color:'+(value.moreTextColor)+';'}}">{{value.listMore.title}}</view></view></block><view class="diy-bargain-list"><block wx:for="{{$root.l0}}" wx:for-item="dataItem" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['dataList','',index]]]]]]]}}" class="bargin-item" bindtap="__e"><image src="{{dataItem.g1}}" mode="aspectFit" data-event-opts="{{[['error',[['imageError',[index]]]]]}}" binderror="__e"></image><view class="content"><view class="title">{{dataItem.$orig.sku_name}}</view><view class="operation"><view class="price-box"><text class="original-price">{{dataItem.$orig.price}}</text><text><block wx:if="{{dataItem.$orig.floor_price=='0.00'}}"><block><text class="floor-price ns-text-color ns-font-size-lg">{{"最低价 "+dataItem.m0+"0.00"}}</text></block></block><block wx:else><block><text class="floor-price ns-text-color ns-font-size-lg">{{"最低价 "+dataItem.m1+dataItem.$orig.floor_price}}</text></block></block></text></view><button class="btn ns-bg-color">去砍价</button></view></view></view></block></view></view></block>