.account-list-content{margin:20rpx 30rpx;padding:0 30rpx;background-color:#fff;-webkit-border-radius:10rpx;border-radius:10rpx}.account-list-content .edit-item{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center;padding:0 30rpx;padding:30rpx 0;background-color:#fff}.account-list-content .edit-item .tit{width:120rpx}.account-list-content .edit-item .desc{-webkit-box-flex:1;-webkit-flex:1;flex:1;margin-left:20rpx;padding:0}.account-list-content .edit-item:first-of-type{margin-top:20rpx}.account-list-content .edit-item .picker{-webkit-box-flex:1;-webkit-flex:1;flex:1}.account-list-content .edit-item .picker text:last-child{line-height:50rpx;float:right;color:#909399;font-size:20rpx}.account-list-content > .edit-item + .edit-item{border-top:2rpx solid #eee}.add{margin-top:60rpx;height:80rpx;line-height:80rpx!important;-webkit-border-radius:80rpx;border-radius:80rpx;font-weight:500;width:calc(100% - 60rpx);margin-left:30rpx}.btn{position:fixed;left:0;width:100%;bottom:30rpx;height:auto;padding-bottom:constant(safe-area-inset-bottom);padding-bottom:env(safe-area-inset-bottom)}