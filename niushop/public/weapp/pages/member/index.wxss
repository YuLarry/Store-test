@charset "UTF-8";
/**
 * 你可以通过修改这些变量来定制自己的插件主题，实现自定义主题功能
 * 建议使用scss预处理，并在插件代码中直接使用这些变量（无需 import 这个文件），方便用户通过搭积木的方式开发整体风格一致的App
 */
.container {
  width: 100vw;
  height: calc(100vh - var(--tab-bar-height, 0));
  background: #f8f8f8;
}
.common-wrap {
  width: 100%;
  box-sizing: border-box;
}
.member-info {
  padding-top: 108rpx;
}
.member-info .info-wrap {
  padding: 44rpx 30rpx;
  display: flex;
  align-items: center;
}
.member-info .info-wrap view {
  color: var(--btn-text-color);
}
.member-info .info-wrap .info {
  flex: 1;
  width: 0;
  padding-right: 20rpx;
  overflow: hidden;
}
.member-info .headimg {
  width: 120rpx;
  height: 120rpx;
  overflow: hidden;
  border-radius: 50%;
  margin-right: 20rpx;
}
.member-info .headimg image {
  width: 100%;
}
.member-info .nickname {
  font-weight: bold;
  white-space: nowrap;
  margin-bottom: 0;
  display: flex;
  align-items: center;
}
.member-info .name {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 38rpx;
  font-weight: 600;
}
.member-info .mobile {
  font-size: 26rpx;
  line-height: 1.5;
}
.member-info .desc {
  font-size: 24rpx;
}
.member-info .member-code, .member-info .user-info {
  font-size: 40rpx;
  color: #fff;
  margin-left: 20rpx;
  line-height: 1;
  color: var(--btn-text-color);
}
.member-info .member-level {
  font-size: 24rpx;
  background: linear-gradient(107deg, #7c7878 0%, #201a18 100%);
  color: #f7c774;
  line-height: 40rpx;
  height: 40rpx;
  border-radius: 4rpx;
  padding: 0 16rpx;
  display: inline-block;
  margin-left: 20rpx;
}
.member-info .member-level .icondiy {
  line-height: 1;
  font-size: 24rpx;
  margin-right: 10rpx;
}
.member-info .member-level.auth {
  margin-left: 0;
}
.member-info .super-member {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 120rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  background: linear-gradient(107deg, #7c7878 0%, #201a18 100%);
  padding: 30rpx 40rpx;
  box-sizing: border-box;
}
.member-info .super-member .icon-huangguan {
  margin-right: 10rpx;
  font-size: 40rpx;
}
.member-info .super-member .super-info {
  flex: 1;
  width: 0;
  font-size: 36rpx;
  color: var(--super-member-start-text-color);
  display: flex;
  align-items: center;
}
.member-info .super-member .see {
  line-height: 1;
  font-size: 30rpx;
}
.member-info .super-member .iconright {
  font-size: 28rpx;
  margin-left: 10rpx;
}
.account-info {
  display: flex;
  padding: 40rpx 0;
  align-items: center;
  justify-content: center;
}
.account-info view {
  color: #fff;
}
.account-info .solid {
  height: 70rpx;
  width: 2rpx;
  background: #fff;
  border-radius: 2rpx;
}
.account-info .account-item {
  flex: 1;
  text-align: center;
}
.account-info .account-item .value {
  font-size: 34rpx;
  font-weight: bold;
  margin-bottom: 4rpx;
  line-height: 1.3;
}
.account-info .account-item .title {
  font-size: 26rpx;
}
.info-wrap[data-style="1"] .super-text {
  background-image: linear-gradient(90deg, #ffdba6 0%, #ffebca 49%, #f7c774 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  display: flex;
  align-items: center;
}
.info-wrap[data-style="2"] .member-info {
  border-radius: 0 0 100% 100%/0 0 70rpx 70rpx;
  overflow: hidden;
}
.info-wrap[data-style="2"] .member-info .member-level {
  background: linear-gradient(107deg, #FADCB5 0%, #F6BD74 100%);
  color: #8D4B16;
}
.info-wrap[data-style="2"] .account-info {
  background: #fff;
  margin: 20rpx 0 0 0;
  color: #333;
  border-radius: 18rpx;
}
.info-wrap[data-style="2"] .account-info .solid {
  background: #f2f2f2;
}
.info-wrap[data-style="2"] .account-info .account-item .value {
  color: #000000;
}
.info-wrap[data-style="2"] .account-info .account-item .title {
  color: #666666;
}
.info-wrap[data-style="2"] .super-member {
  color: #8d4b16;
  background: linear-gradient(107deg, #fadcb5 0%, #f6bd74 100%);
  border-top-left-radius: 18rpx;
  border-top-right-radius: 18rpx;
}
.info-wrap[data-style="2"] .super-member .super-info {
  color: var(--super-member-start-text-color);
}
.info-wrap[data-style="2"] .super-member .see {
  color: var(--super-member-start-text-color);
}
.info-wrap[data-style="2"] .super-member .iconright {
  color: var(--super-member-start-text-color);
}
.order-wrap {
  background: #fff;
}
.order-wrap .status-wrap {
  display: flex;
  padding: 30rpx 0;
  align-items: center;
  justify-content: center;
  color: #333;
}
.order-wrap .item-wrap {
  flex: 1;
  text-align: center;
}
.order-wrap .item-wrap .icon-block {
  width: 60rpx;
  height: 60rpx;
  font-size: 60rpx;
  margin: 4rpx auto;
  position: relative;
}
.order-wrap .item-wrap .order-num {
  position: absolute;
  top: 0;
  right: 0;
  -webkit-transform: translate(50%, -50%);
          transform: translate(50%, -50%);
  display: inline-block;
  box-sizing: border-box;
  color: #FFFFFF;
  line-height: 1.2;
  text-align: center;
  font-size: 24rpx;
  padding: 0 6rpx;
  min-width: 30rpx;
  border-radius: 16rpx;
  height: 30rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.order-wrap .item-wrap .title {
  font-size: 26rpx;
}
.order-wrap .head-wrap {
  display: flex;
  padding: 15px 0;
  margin: 0 30rpx;
  align-items: center;
  border-bottom: 2rpx solid #eeeeee;
}
.order-wrap .head-wrap .title {
  flex: 1;
  width: 0;
  font-size: 34rpx;
  font-weight: bold;
}
.order-wrap .head-wrap .see, .order-wrap .head-wrap .iconright {
  line-height: 1;
  color: #999;
}
.order-wrap .head-wrap .iconright {
  font-size: 28rpx;
  margin-left: 10rpx;
}
.menu-wrap {
  background: #fff;
}
.menu-wrap .head-wrap {
  display: flex;
  padding: 30rpx 0;
  margin: 0 30rpx;
  align-items: center;
}
.menu-wrap .head-wrap .title {
  flex: 1;
  width: 0;
  font-size: 34rpx;
  font-weight: bold;
}
.menu-wrap .menu-list {
  display: flex;
  padding-bottom: 30rpx;
  color: #333;
  flex-wrap: wrap;
}
.menu-wrap .item-wrap {
  width: 25%;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.menu-wrap .item-wrap .icon {
  width: 60rpx;
  height: 60rpx;
}
.menu-wrap .item-wrap .icon-block {
  width: inherit;
  height: inherit;
  font-size: 60rpx;
}
.menu-wrap .item-wrap .title {
  font-size: 26rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-top: 12rpx;
  width: 100%;
  text-align: center;
}
.menu-wrap .icon image {
  width: 100%;
  height: auto;
}
[menu-type="list"] .item-wrap {
  width: 100%;
  flex-direction: row;
  padding: 20rpx 30rpx;
}
[menu-type="list"] .item-wrap .title {
  flex: 1;
  margin-top: 0;
  text-align: left;
  padding: 0 20rpx;
  line-height: 1;
}
.adv-wrap {
  width: 100%;
  overflow: hidden;
  position: relative;
  z-index: 1;
}
.adv-wrap .swiper-wrap {
  width: 100%;
  height: 100%;
}
.adv-wrap .swiper-item {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.adv-wrap .swiper-item image {
  width: 100%;
}
.member-code-popup {
  width: 100%;
  min-height: 900rpx;
  background: none;
}
.member-code-popup .popup-top {
  padding: 40rpx;
  box-sizing: border-box;
  width: 100%;
  height: 800rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
}
.member-code-popup .popup-top .popup-top-title {
  display: flex;
  align-items: center;
  margin-bottom: 60rpx;
}
.member-code-popup .popup-top .popup-top-title .popup-top-title-txt {
  font-size: 30rpx;
  margin-left: 16rpx;
}
.member-code-popup .popup-top .popup-top-tiao {
  width: 480rpx;
  height: 160rpx;
  margin: 0 auto 46rpx;
}
.member-code-popup .popup-top .popup-top-tiao image {
  width: 100%;
  height: 100%;
}
.member-code-popup .popup-top .popup-top-tiaoJie {
  text-align: center;
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 40rpx;
}
.member-code-popup .popup-top .popup-top-erwei {
  width: 254rpx;
  height: 254rpx;
  margin: 0 auto 20rpx;
}
.member-code-popup .popup-top .popup-top-erwei image {
  width: 100%;
  height: 100%;
}
.member-code-popup .popup-top .popup-top-shauxin {
  width: 350rpx;
  height: 80rpx;
  border-radius: 10rpx;
  background: #f5f9f7;
  margin: 0 auto;
  font-size: 30rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.member-code-popup .popup-top .popup-top-shauxin ._i {
  margin-left: 20rpx;
}
.member-code-popup .popup-top .popup-top-text {
  text-align: center;
  font-size: 18rpx;
  color: #999999;
  margin-top: 30rpx;
}
.member-code-popup .popup-bottom {
  display: flex;
  align-items: center;
  justify-content: center;
  background: none !important;
}
.member-code-popup .popup-bottom .iconfont-delect {
  font-size: 60rpx;
  margin-top: 20rpx;
  color: white;
}
 .uni-popup__wrapper.uni-custom .uni-popup__wrapper-box {
  background: none !important;
}
.member-code-popup .popup-top {
  height: auto;
}

