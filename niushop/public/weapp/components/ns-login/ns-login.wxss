.uni-tip {
  width: 580rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 10rpx;
  overflow: hidden;
  height: initial;
}
.uni-tip-title {
  text-align: center;
  font-weight: bold;
  font-size: 32rpx;
  color: #303133;
  padding-top: 50rpx;
}
.uni-tip-content {
  padding: 0 30rpx;
  color: #606266;
  font-size: 28rpx;
  text-align: center;
}
.uni-tip-icon {
  width: 100%;
  text-align: center;
  margin-top: 60rpx;
}
.uni-tip-icon image {
  width: 300rpx;
}
.uni-tip-group-button {
  margin-top: 30rpx;
  line-height: 120rpx;
  padding: 0 50rpx 50rpx 50rpx;
}
.uni-tip-button {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border: none;
  border-radius: 80rpx;
  padding: 0 !important;
  margin: 20rpx 0 0 0 !important;
  background: var(--base-color);
  font-size: 28rpx;
}
.uni-tip-group-button .close {
  color: #999;
  font-size: 28rpx;
  text-align: center;
  margin-top: 20rpx;
}
.uni-tip-button:after {
  border: none;
}


